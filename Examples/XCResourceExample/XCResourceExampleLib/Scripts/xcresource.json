{
    "commands": [
        {
            "commandName": "xcassets2swift",
            "xcassetsPaths": [
                "Sources/Resources/Resources/Assets.xcassets"
            ],
            "assetTypes": ["imageset"],
            "swiftPath": "Sources/Resources/ResourceAccess/ImageKey.swift",
            "keyTypeName": "ImageKey",
            "accessLevel": "public"
        },
        {
            "commandName": "xcassets2swift",
            "xcassetsPaths": [
                "Sources/Resources/Resources/Assets.xcassets"
            ],
            "assetTypes": ["colorset"],
            "swiftPath": "Sources/Resources/ResourceAccess/ColorKey.swift",
            "keyTypeName": "ColorKey",
            "accessLevel": "public"
        },
        {
            "commandName": "files2swift",
            "resourcesPath": "Sources/Resources/Resources/Fonts",
            "filePattern": "(?i)\\.(otf|ttf|ttc)$",
            "swiftPath": "Sources/Resources/ResourceAccess/FileResource.swift",
            "keyTypeName": "FileResource",
            "preservesRelativePath": true,
            "relativePathPrefix": "Fonts",
            "bundle": "Bundle.module",
            "accessLevel": "public"
        },
        {
            "commandName": "fonts2swift",
            "resourcesPath": "Sources/Resources/Resources",
            "swiftPath": "Sources/Resources/ResourceAccess/FontResource.swift",
            "keyTypeName": "FontResource",
            "keyListName": "all",
            "generatesLatinKey": true,
            "stripsCombiningMarksFromKey": true,
            "preservesRelativePath": true,
            "bundle": "Bundle.module",
            "accessLevel": "public"
        },
        {
            "commandName": "xcstrings2swift",
            "catalogPath": "Sources/Resources/Resources/Localizable.xcstrings",
            "bundle": "at-url:Bundle.module.bundleURL",
            "swiftPath": "Sources/Resources/ResourceAccess/LocalizedStringResource+.swift"
        }
    ]
}
